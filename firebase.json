{
  "hosting": {
    "public": "public",
    "ignore": ["**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      { "source": "/api/**", "run": { "serviceId": "console-api", "region": "me-central1" } },
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
